{
  "name": "@jahed/slack-utilities",
  "version": "0.4.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "docs"
  ],
  "author": "Jahed Ahmed",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "lint": "tslint --project ./tsconfig.json",
    "test": "jest",
    "build:lib": "rimraf lib && tsc",
    "build:docs": "rimraf docs && typedoc --theme default --exclude '**/*.test.ts' --out docs --mode file --target ES6",
    "build": "yarn build:lib && yarn build:docs",
    "push:git": "git push && git push --tags",
    "push:docs": "bash -i ./publish-docs.sh",
    "prepublishOnly": "yarn lint && yarn test && yarn build",
    "postpublish": "yarn push:git && yarn push:docs"
  },
  "dependencies": {
    "node-fetch": "2.2.0",
    "qs": "6.5.2"
  },
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/nock": "9.3.0",
    "@types/node-fetch": "2.1.2",
    "@types/qs": "6.5.1",
    "jest": "23.6.0",
    "nock": "10.0.1",
    "rimraf": "2.6.2",
    "ts-jest": "23.10.4",
    "tslint": "5.11.0",
    "tslint-config-standard": "8.0.1",
    "typedoc": "0.13.0",
    "typescript": "3.1.6"
  }
}
