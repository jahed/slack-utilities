{
  "name": "@jahed/slack-utilities",
  "version": "0.1.2",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "author": "Jahed Ahmed",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "prepublish": "yarn test && yarn run build",
    "postpublish": "git push --follow-tags",
    "docs": "bash -i ./docs.sh"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "node-fetch": "1.7.3",
    "qs": "6.5.1"
  },
  "devDependencies": {
    "@types/jest": "21.1.7",
    "@types/nock": "8.2.1",
    "@types/node-fetch": "1.6.7",
    "@types/qs": "6.5.1",
    "jest": "21.2.1",
    "nock": "9.1.0",
    "ts-jest": "21.2.3",
    "typedoc": "0.9.0",
    "typescript": "2.6.1"
  }
}
